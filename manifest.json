{
  "manifest_version": 2,
 
  "name": "Enigma",
  "version": "0.2.0",
  "description": "Expand your horizons.",
  "browser_action": {
      "default_icon": "map.png"
},
  "background": {
    "scripts": ["background.js", "jquery-1.12.4.min.js"],
    "persistent": true
  },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*/"
    ]
}