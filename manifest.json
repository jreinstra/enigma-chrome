{
  "manifest_version": 2,
 
  "name": "John and Jason's Fun Ride",
  "version": "0.2.0",
  "description": "cancer?",
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-1.12.4.min.js", "content.js"],
    "matches": ["https://*.vox.com/*", "https://*.huffingtonpost.com/*", "http://*.breitbart.com/*", "https://*.theblaze.com/*", "https://*.foxnews.com/*", "https://*.cnn.com/*", "https://*.wsj.com/*", "https://*.realclearpolitics.com/*", "https://*.forbes.com/*", "https://*.nbcnews.com/*", "https://*.cbsnews.com/*", "https://*.bloomberg.com/*", "https://*.politico.com/*", "https://*.thewashingtonpost.com/*", "https://*.nytimes.com/*", "https://*.nypost.com/*", "https://*.bluelivesmatter.blue/*"],
      "run_at":    "document_start"
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*/"
    ]
}